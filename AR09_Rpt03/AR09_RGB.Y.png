<script type="text/template" id="composerHotIssueTemplate">
    <% if(hotIssues.length !== 0) { %>
        <div class="search_box">
            <strong class="ico_mailsearch tit_news">실시간 이슈</strong>
            <!--2017-05-10 텍스트 수정-->
            <ul class="tab_aside tab_media">
                <% for(var issue, len = hotIssues.length, i = 0; issue = hotIssues[i], i < len; i++) { ;%>
                <li data-index="<%- i %>">
                    <a href="javascript:;" class="ico_mailsearch link_tab<%- (i+1) %>">
                        <%- issue.name %>
                    </a>
                    <div class="cont_aside">
                        <strong class="screen_out">
                            <%- issue.name %>
                        </strong>
                        <!--선택된 탭의 메뉴 명이 들어갑니다.-->
                        <ol class="list_ranking">
                            <% for(var item, jlen = (issue.data.length > 7 ? 7 : issue.data.length), j = 0; item = issue.data[j], j < jlen; j++) { %>
                            <li>
                                <em class="ico_mailsearch num<%- (j+1) %>"><%- (j+1) %>></em>
                                <span class="screen_out">위</span>
                                <strong class="tit_g">
                                    <a href="javascript:;" target="_blank" class="link_txt" data-keyword="<%- item.keyword %>">
                                        <%- item.keyword %>
                                    </a>
                                </strong>
                                <span class="info_status">
                                    <span class="screen_out">순위</span>
                                    <%
                                    switch(item.type) {
                                    case "new":
                                    %>
                                     <span class="ico_mailsearch ico_new">
                                         <%- item.value %>
                                         <span class="screen_out">
                                             새로진입
                                         </span>
                                     </span>
                                    <%
                                    break;
                                    case "+":
                                    %>
                                     <span class="ico_mailsearch ico_up">
                                         <%- item.value %>
                                         <span class="screen_out">
                                             하락
                                         </span>
                                     </span>
                                    <%
                                    break;
                                    case "-":
                                    %>
                                     <span class="ico_mailsearch ico_down">
                                         <%- item.value %>
                                         <span class="screen_out">
                                             하락
                                         </span>
                                     </span>
                                    <%
                                    break;
                                    default:
                                    %>
                                     <span class="ico_mailsearch ico_keep">
                                         <%- item.value %>
                                         <span class="screen_out">
                                             유지
                                         </span>
                                     </span>
                                    <%
                                    break;
                                    } // End of switch
                                    %>
                                </span>
                            </li>
                            <% } // End of for %>
                        </ol>
                    </div>
                </li>
                <% } // End of For %>
            </ul>
        </div>
    <% } // End of If %>
</script>

<script type="text/template" id="composerPreviewTemplate">
    <strong class="tit_preview">미리보기</strong>
    <div class="mail_preview">
        <div class="preview_header">
            <strong class="tit_view"><%- subject %></strong>
            <dl class="list_info">
                <dt>보낸사람</dt>
                <dd>
                    <%- from.name %> &lt;<%- from.addr %>&gt;
                </dd>
                <dt>받는사람</dt>
                <dd>
                    <% for(var i = 0;i < toList.length;i++){ %>
                        <%- toList[i].name %>&lt;<%- toList[i].addr %>&gt;<% if(i !== (toList.length - 1)){ %>,<% } %>
                    <% } %>
                </dd>
                <dt>참조</dt>
                <dd>
                    <% for(var i = 0;i < ccList.length;i++){ %>
                        <%- ccList[i].name %>&lt;<%- ccList[i].addr %>&gt;<% if(i !== (ccList.length - 1)){ %>,<% } %>
                    <% } %>
                </dd>
                <dt>숨은참조</dt>
                <dd>
                    <% for(var i = 0;i < bccList.length;i++){ %>
                    <%- bccList[i].name %>&lt;<%- bccList[i].addr %>&gt;<% if(i !== (bccList.length - 1)){ %>,<% } %>
                    <% } %>
                </dd>
            </dl>

            <dl class="list_attach">
                <dt>첨부파일</dt>
                <dd>
                    <% if(attachments.normalFileCount > 0 || attachments.bigFileCount > 0){ %>
                        <% if(attachments.normalFileCount > 0){ %>
                        <span class="txt_attach"><span class="img_mail ico_file"></span><em class="emph_file">일반 첨부파일 <%= attachments.normalFileCount %>개</em> (<%= attachments.normalFileSize %>)</span>
                        <% } %>

                        <% if(attachments.normalFileCount > 0 && attachments.bigFileCount > 0){ %>
                        <span class="txt_bar"></span>
                        <% } %>

                        <% if(attachments.bigFileCount > 0){ %>
                        <span class="txt_attach"><span class="img_mail ico_file"></span><em class="emph_file">대용량 첨부파일 <%= attachments.bigFileCount %>개</em> (<%= attachments.bigFileSize %>)</span>
                        <% } %>
                    <% }else{ %>
                    첨부된 파일이 없습니다.
                    <% } %>
                </dd>
            </dl>

        </div>
        <div class="preview_body">
            <%= contents %>
        </div>
    </div>

</script>

