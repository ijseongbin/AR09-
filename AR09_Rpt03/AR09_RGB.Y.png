<script type="text/template" id="mailsToolbarTemplate">
    <%
        var btn_forwards = '<button type="button" class="btn_toolbar btn_send"><span class="txt">전달</span></button>';
        var btn_move = '<button type="button" class="btn_toolbar btn_move" aria-haspopup="true"><span class="txt">이동<span class="img_mail ico_arrow"></span></span></button>';
        var btn_edit = '<button type="button" class="btn_toolbar"><span class="txt">수정</span></button>';
        var btn_delete = '<button type="button" class="btn_toolbar btn_del"><span class="txt">삭제</span></button>';
        var btn_permanent = '<button type="button" class="btn_toolbar btn_permanent"><span class="txt">완전삭제</span></button>';
        var btn_blockSpam = '<button type="button" class="btn_toolbar btn_spam"><span class="txt">스팸차단</span></button>';
        var btn_unblockSpam = '<button type="button" class="btn_toolbar btn_unblock_spam"><span class="txt">스팸해제</span></button>';
        var btn_blockReceive = '<button type="button" class="btn_toolbar btn_block"><span class="txt">수신차단</span></button>';
        var btn_empty = '<button type="button" class="btn_toolbar btn_empty"><span class="txt">비우기</span></button>';
        var btn_resend = '<button type="button" class="btn_toolbar btn_resend"><span class="txt">다시보내기</span></button>';
        var btn_flag_read = '<button type="button" class="btn_toolbar btn_misc" aria-haspopup="true"><span class="txt">읽음 표시<span class="img_mail ico_arrow"></span></span></button>';
        var btn_source = '<button type="button" class="btn_toolbar btn_source" aria-haspopup="true">출처별보기<span class="img_mail ico_arrow"></span></button>';
        var btn_refresh = '<button type="button" class="btn_refresh"><span class="img_mail ico_refresh">새로고침</span></button>';
        var link_mailCount = '<span class="link_read"><a href="javascript:;" class="count_unread"><em class="screen_out">안읽음</em><em class="txt_num"></em></a><span class="txt_slush">/</span><a href="javascript:;" class="count_total"><em class="screen_out">전체</em><em class="total_txt_num"></em></a></span>';
        var txt_bar = '<span class="txt_bar"></span>'
    %>

    <h2 class="screen_out">메일 툴바</h2>
    <div class="wrap_toolbar <% if (folderId === "CMAIL" || folderId === "DRAFT") { %>toolbar_type2<% } %>">
        <div class="select_all">
            <input type="checkbox" id="allCk" name="allCk" class="tf_check">
            <label for="allCk" class="lab_check"><span class="img_mail ico_check">메일 전체선택</span></label><!-- on클래스로 제어  -->
        </div>

		<% if (folderId === "SPAM") { %>
            <div class="wrap_bold">
                <%= [btn_permanent, btn_unblockSpam, btn_blockSpam, btn_empty].join("") %>
            </div>
            <%= txt_bar %>
            <%= [btn_flag_read, link_mailCount].join("") %>

		<% } else if (folderId === "TRASH") { %>
            <div class="wrap_bold">
                <%= [btn_permanent, btn_blockSpam, btn_empty].join("") %>
            </div>
            <%= txt_bar %>
            <%= [btn_move, btn_flag_read, link_mailCount].join("") %>

        <% } else if (folderId === "SCRAP") { %>
            <div class="wrap_bold">
                <%= [btn_permanent].join("") %>
            </div>
            <%= txt_bar %>
            <%= [btn_flag_read].join("") %>

		<% } else if (folderId === "SENT") { %>
            <div class="wrap_bold">
                <%= [btn_delete, btn_forwards, btn_resend].join("") %>
            </div>
            <%= txt_bar %>
            <%= [btn_move].join("") %>

		<% } else if (folderId === "DRAFT" || folderId === "RESERVE") { %>
            <div class="wrap_bold">
                <%= [btn_permanent].join("") %>
            </div>

        <% } else if (folderId === "CMAIL") { %>
            <div class="wrap_bold">
                <%= [btn_delete].join("") %>
            </div>

        <% } else if (folderId === "MINE") { %>
            <div class="wrap_bold">
                <%= [btn_delete, btn_forwards].join("") %>
            </div>
            <%= txt_bar %>
            <%= [btn_move, btn_flag_read, link_mailCount].join("") %>

        <% } else if (["UNREAD", "STAR", "ATTACH"].indexOf(folderId) > -1) { %>
            <div class="wrap_bold">
                <%= [btn_delete, btn_blockSpam, btn_forwards].join("") %>
            </div>
            <%= txt_bar %>
            <%= [btn_move, btn_flag_read].join("") %>

        <% } else if (folderId === "SEARCH") { %>
            <div class="wrap_bold">
                <%= [btn_delete, btn_forwards].join("") %>
            </div>
            <%= txt_bar %>
            <%= [btn_move, btn_flag_read].join("") %>
        <% } else { %>
            <div class="wrap_bold">
                <%= [btn_delete, btn_blockSpam, btn_forwards].join("") %>
            </div>
            <%= txt_bar %>
            <%= [btn_move, btn_flag_read, link_mailCount].join("") %>

    	<% } %>

        <%= btn_refresh %>

        <% if (folderId === "CMAIL") { %>
            <%= txt_bar %>
            <div class="info_toolbar">
                수신 확인 정보 3개월 자동보관<span class="img_mail ico_info"></span>
                <div class="layer_info">
                    <ul class="list_info">
                        <li>- 수신확인 정보는 <span class="emph_color">3개월</span>동안 보관됩니다.</li>
                        <li>- 텍스트모드로 발송한 메일, 받는 사람의 메일 설정이 '받은 메일 이미지 안보기'인 경우에는 수신확인을 할 수 없습니다.</li>
                        <li>- 발송취소는 Daum메일 회원에게 보낸경우에 한해 '읽지않음' 상태에서만 가능합니다.<br>(단, 상대방이 휴대폰/메신저 알림 사용중일때 제외)</li>
                    </ul>
                    <span class="img_mail ico_arrow"></span>
                </div>
            </div>
        <% } %>

    </div>
</script>

