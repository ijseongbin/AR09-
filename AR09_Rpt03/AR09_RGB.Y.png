<script type="text/template" id="mailsItemTemplate">
<% _.each(data.mails, function(mail){ %>
<li class="mail_item <%= mail.cmailUnreadStatus %>" data-mailid=<%= mail.id %>>
    <span class="img_mail ico_choice">선택됨</span>
    <% if (mail.folderId === "RESERVE"){ %>
        <% if(mail.reservationFailureMail){ %>
            <strong class="tit_reservation txt_infeasible">발송불가</strong>
        <% }else if(mail.reservationWaitMail){ %>
            <strong class="tit_reservation">발송대기</strong>
        <% }else{ %>
            <strong class="tit_reservation emph_color">발송완료</strong>
        <% } %>
    <% } else if(mail.folderId === "CMAIL") { %>
        <% if(mail.recipients.length > 1){ %>
            <% if(mail.recipients.length === mail.viewedLength) { %>
                <strong class="txt_chek">읽음</strong>
            <% } else if(mail.viewedLength === 0) { %>
                <strong class="txt_chek emph_color">읽지않음</strong>
            <% } else { %>
                <strong class="txt_chek"><%= mail.viewedLength %>명 읽음</strong>
            <% } %>
        <% }else if(mail.recipients.length === 1){ %>
            <% var checkStatus = mail.recipients[0].checkStatus %>
            <% if(checkStatus === "NEW"){ %>
                <strong class="txt_chek emph_color">읽지않음</strong>
            <% }else if(checkStatus === "IMPOSSIBLE"){ %>
                <strong class="txt_chek txt_cancel">수신확인불가</strong>
            <% }else if(checkStatus === "CANCELED"){ %>
                <strong class="txt_chek txt_cancel">발송취소</strong>
            <% }else if(checkStatus === "REJECTED"){ %>
                <strong class="txt_chek txt_cancel">반송됨</strong>
            <% }else if(checkStatus === "FAILED"){ %>
                <strong class="txt_chek txt_cancel">발송실패</strong>
            <% }else if(checkStatus === "SENDING"){ %>
                <strong class="txt_chek emph_color">발송중</strong>
            <% }else if(checkStatus === "VIEWED"){ %>
                <strong class="txt_chek">읽음</strong>
            <% } %>
        <% } else { %>
            <strong class="txt_chek">삭제됨</strong>
        <% } %>
    <% } %>
    <div class="info_mail">
        <input type="checkbox" id="mCk" name="mCk" class="tf_check" <%= mail._isChecked === true ? "checked" : "" %> title="<%- mail.toName %> <%- mail.subjectForList %>">
        <label aria-hidden="true" for="mCk" class="lab_check <%= mail._isChecked === true ? "on" : "" %>" tabindex="0"><span class="img_mail ico_check"></span></label>
        <% if (mail.folderId !== "TRASH" && mail.folderId !== "SPAM" && mail.folderId !== "DRAFT" && mail.folderId !== "CMAIL" && mail.folderId !== "RESERVE") { %>
            <a href="javascript:;" class="btn_important <%= mail.isStar === true ? "on" : "" %>">
                <span class="img_mail ico_star">중요메일표시 <% if(mail.isStar === true) { %> 끄기 <% }else{ %> 켜기 <% } %></span>
            </a>
        <% } %>
        <% if (mail.folderId !== "SENT" && mail.folderId !== "CMAIL" && mail.folderId !== "RESERVE") { %>
            <a href="javascript:;" class="btn_read <%= mail.isUnread === true ? "on" : "" %>">
                <span class="img_mail <%= mail.mailIconClassName %>"><%= mail.mailIconMsg %></span>
            </a>
        <% } %>
        <a href="javascript:;" class="btn_del"><span class="img_mail ico_del">메일 삭제</span></a>
        <% if (mail.isAttached){ %>
            <span class="img_mail ico_file">파일첨부</span>
        <% } %>
    </div>
    <% if(mail.folderId === "CMAIL" && mail.recipients.length > 1){ %>
        <button type="button" class="img_mail btn_fold">받는사람 모두 보기</button>
        <% if(mail.recipients.length === mail.viewedLength) {%>
            <strong class="info_from txt_with"><%= mail.recipients.length %>명 모두 읽음</strong>
        <% } else {%>
            <strong class="info_from txt_with"><%= mail.recipients.length %>명 중 <%= mail.viewedLength %>명 읽음</strong>
        <% } %>
    <% } else { %>
        <div class="info_from">
            <% if(mail.folderId === "CMAIL"){ %>
                <% if(mail.recipients.length === 1){ %>
                    <em class="screen_out jeon">받는사람 :</em> <a href="javascript:;" class="link_from" title="<%- mail.titleContents %>"><%- mail.toName %></a>
                <% } else { %>
                <!-- 내가 삭제한 메일 -->
                <% } %>
            <% } else if(mail.originFolderId === "DRAFT" || mail.originFolderId === "RESERVE" || mail.originFolderId === "SENT") { %>
                <em class="screen_out">받는사람 :</em> <a href="javascript:;" class="link_from" title="<%- mail.titleContents %>"><%- mail.toName %></a>
            <% } else { %>
                <em class="screen_out">보낸이 :</em> <a href="javascript:;" class="link_from" title="<%- mail.titleContents %>"><%- mail.toName %></a>
            <% } %>
        </div>
    <% } %>
    <div class="info_subject">
        <% if(mail.deletedMail === false) { %>
            <a href="<%= mail.mailUrl %>" class="link_subject">
                <% if (mail.folderName && mail.folderId !== "CMAIL"){ %>
                    <em class="txt_box"><span class="folder_name"><%= (mail.folderName === "Archive") ? "보관메일함" : mail.folderName %></span><span class="bg_frame"></span></em>
                <% } %>
                <% if(mail.folderId !== "SENT" && mail.originCode && mail.originCode === "1"){ %>
                    <em class="txt_box">보낸메일<span class="bg_frame"></span></em>
                <% } %>
                <strong class="tit_subject">
                    <% if (mail.folderId === "SEARCH") { %>
                        <%= mail.subjectForList %>
                    <% } else { %>
                        <%- mail.subjectForList %>
                    <% } %>
                </strong>
            </a>
            <% if (mail.originFolderId !== "DRAFT") { %>
                <a href="<%= mail.newWindowMailUrl %>" class="link_window" target="_blank"><span class="img_mail ico_widow read_new_window">새창으로 읽기</span></a>
            <% } %>
        <% } else { %>
            <span href="javascript:;" class="link_subject"><strong class="tit_subject"><%- mail.subjectForList %></strong></span>
        <% } %>
    </div>

    <% if(mail.folderId === "CMAIL"){ %>
        <span class="txt_date"><em class="screen_out">보낸날짜 :</em><%= mail.sendDate %></span>
        <% if(mail.recipients.length <= 1){ %>

            <% if(checkStatus === "NEW" && mail.recipients[0].cancelable){ %>
                <span class="txt_date txt_read">
                    <button type="button" class="btn_cancel"><span class="txt">발송취소</span></button>
                </span>
            <% }else if(mail.recipients[0] && mail.recipients[0].checkTimeMoment){ %>
                <span class="txt_date txt_read"><em class="screen_out">발생시간 :</em><%= mail.recipients[0].checkTimeMoment.format("YY.MM.DD HH:mm") %></span>
            <% } %>
        <% } %>
    <% } else { %>
        <span class="txt_date">
        <em class="screen_out"><% if(mail.folderId === "RESERVE"){ %>작성날짜 :<% }else{ %>받은날짜 :<% } %></em>
        <% if(mail.folderId === "RESERVE"){ %>
            <%= mail.reserveSendDate %>
        <% } else if (!mail.recvDate) { %>
            <%= mail.sendDate %>
        <% } else { %>
            <%= mail.recvDate %>
        <% } %>
    </span>
    <% } %>
