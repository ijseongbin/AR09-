<script type="text/template" id="viewerToolbarTemplate">
<%
    var btn_reply = '<button type="button" class="btn_toolbar btn_reply"><span class="txt">답장</span></button>';
    var btn_replyAll = '<button type="button" class="btn_toolbar btn_allreply"><span class="txt">전체답장</span></button>';
    var btn_forwards = '<button type="button" class="btn_toolbar btn_forward"><span class="txt">전달</span></button>';
    var btn_edit = '<button type="button" class="btn_toolbar btn_edit"><span class="txt">수정</span></button>';
    var btn_editReserved = '<button type="button" class="btn_toolbar btn_editReserved"><span class="txt">수정</span></button>';
    var btn_delete = '<button type="button" class="btn_toolbar btn_del"><span class="txt">삭제</span></button>';
    var btn_permanent = '<button type="button" class="btn_toolbar btn_permanent"><span class="txt">완전삭제</span></button>';
    var btn_resend = '<button type="button" class="btn_toolbar btn_resend"><span class="txt">다시보내기</span></button>';
    var btn_blockSpam = '<button type="button" class="btn_toolbar btn_spam"><span class="txt">스팸차단</span></button>';
    var btn_unblockSpam = '<button type="button" class="btn_toolbar btn_unblock_spam"><span class="txt">스팸해제</span></button>';
    var btn_blockReceive = '<button type="button" class="btn_toolbar btn_block_receive"><span class="txt">수신차단</span></button>';
    var btn_move = '<button type="button" class="btn_toolbar btn_move" aria-haspopup="true"><span class="txt">이동<span class="img_mail ico_arrow"></span></span></button>';
    var btn_misc = '<button type="button" class="btn_toolbar btn_misc" aria-haspopup="true"><span class="txt">추가기능<span class="img_mail ico_arrow"></span></span></button>';
    var txt_bar = '<span class="txt_bar"></span>';
%>
<div class="wrap_toolbar">
    <% if (folderId === "TRASH") { %>
        <div class="wrap_bold">
            <%= [btn_reply, btn_replyAll, btn_forwards, btn_permanent, btn_blockReceive].join("") %>
        </div>
        <%= txt_bar %>
        <%= [btn_move, btn_misc].join("") %>

    <% } else if (folderId === "SPAM") { %>
        <div class="wrap_bold">
            <%= [btn_reply, btn_replyAll, btn_forwards, btn_permanent, btn_unblockSpam, btn_blockSpam].join("") %>
        </div>
        <%= txt_bar %>
        <%= [btn_misc].join("") %>

    <% } else if (folderId === "RESERVE") { %>
        <div class="wrap_bold">
            <% if (labelIds.indexOf("RESERVE_WAIT") > -1) { %>
            <%= [btn_permanent, btn_editReserved].join("") %>
            <% } else { %>
            <%= [btn_permanent].join("") %>
            <% } %>
        </div>
        <%= txt_bar %>
        <%= [btn_misc].join("") %>

    <% } else if (folderId === "SENT") { %>

        <div class="wrap_bold">
            <%= [btn_reply, btn_replyAll, btn_forwards, btn_delete, btn_resend].join("") %>
        </div>
        <%= txt_bar %>
        <%= [btn_move, btn_misc].join("") %>

    <% } else if (folderId === "CMAIL") { %>
        <div class="wrap_bold">
            <%= [btn_reply, btn_replyAll, btn_forwards, btn_delete, btn_resend].join("") %>
        </div>
        <%= txt_bar %>
        <%= [btn_misc].join("") %>

    <% } else if (folderId === "MINE") { %>

        <div class="wrap_bold">
            <%= [btn_forwards, btn_edit, btn_delete].join("") %>
        </div>
        <%= txt_bar %>
        <%= [btn_move, btn_misc].join("") %>

    <% } else if (folderId === "SCRAP") { %>

        <div class="wrap_bold">
            <%= [btn_permanent].join("") %>
        </div>
        <%= txt_bar %>
        <%= [btn_misc].join("") %>

    <% } else { %>

        <div class="wrap_bold">
            <%= [btn_reply, btn_replyAll, btn_forwards, btn_delete, btn_blockSpam].join("") %>
        </div>
        <%= txt_bar %>
        <%= [btn_move, btn_misc].join("") %>

    <% } %>
</div>
</script>


