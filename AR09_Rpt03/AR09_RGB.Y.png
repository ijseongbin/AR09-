<script type="text/template" id="composerTemplate">
    <div class="inner_write">
    		<h3 class="screen_out">메일쓰기</h3>
    		<div class="box_minimized"><!--  마우스 오버시 desc_on 클래스추가 -->
    			<div class="desc_minimized">
					<h4 class="tit_info">
						<a href="javascript:;" class="link_mail">
							<span class="img_mail ico_notice"></span>
							<span class="tit_minimized">새 메일</span>
						</a>
					</h4>
					<a href="javascript:;" class="btn_close"><span class="img_mail ico_close_type2">취소</span></a>
                </div>
                <p class="desc_mail">
					<span class="txt_mail"></span>
                </p>
            </div>
    		<div class="cont_toolbar">
    			<div class="inner_toolbar">
    				<div class="wrap_toolbar">
                        <% if (reservationTime && fromMailId) { %>
                        <button type="button" class="btn_toolbar btn_write btn_write_reserved"><span class="txt">예약메일 저장</span></button>
                        <% } else { %>
                        <button type="button" class="btn_toolbar btn_write"><span class="txt">보내기</span></button>
                        <button type="button" class="btn_toolbar btn_save"><span class="txt">임시저장</span></button>
                        <% } %>
						<button type="button" class="btn_toolbar btn_preview"><span class="txt">미리보기</span></button>
						<span class="txt_bar"></span>

						<div class="select_me writeMe <% if(saveToMe){ %>on<% } %>"><!-- 클래스 on으로 제어 -->
							<input type="checkbox" id="writeMe" name="writeMe" class="tf_check" <% if(saveToMe){ %>checked<% } %>>
							<label for="writeMe" class="lab_check <% if(saveToMe){ %>on<% } %>">
								<span class="img_mail ico_check"></span>
								<span class="lab_txt">내게쓰기</span>
							</label>
						</div>
						<span id="autosaveTime" class="txt_save" style="display:none;"></span>
                    </div>
    			</div>
    		</div>
    		<div class="compose_mail" <% if (newWindow) { %>style="right:0;"<% } %>>
                <% if (!newWindow) { %>
                <div class="wrap_mode">
                    <div class="inner_mode">
                        <strong class="screen_out">메일쓰기 창 크기조절</strong>
                        <button type="button" class="btn_action btn_minimize" title="최소화"><span class="img_mail ico_minimize">최소화</span></button>
                        <button type="button" class="btn_action btn_full" title="늘리기"><span class="img_mail ico_full">늘리기</span></button>
                        <button type="button" class="btn_action btn_compact" title="줄이기"><span class="img_mail ico_compact">줄이기</span></button>
                        <button type="button" class="btn_action btn_new" title="새창쓰기"><span class="img_mail ico_new">새창쓰기</span></button>
                        <button type="button" class="btn_action btn_cancel" title="닫기"><span class="img_mail ico_cancel">닫기</span></button>
                    </div>
                </div>
                <% } %>
                <div class="wrap_composer">
                    <dl class="info_write">
                        <dt class="tit_to">보내는사람</dt>
                        <dd id="fromAccounts" class="desc_to">
                            <div class="desc_write">
                                <div class="box_name"><!-- 이름 클릭시 on 클래스추가 -->
                                    <input type="text" class="inp_name" id="toName" name="toName" title="보내는 사람 이름입력" value="<%- from.name %>">
                                    <span class="txt_name"><%- from.name %></span>
                                    <a href="javascript:;" class="link_close"><span class="img_mail">보내는 사람 이름 삭제</span></a>
                                </div>
                                <div class="desc_writer" style="position:relative;">
                                    <a href="javascript:;" class="link_writer" aria-haspopup="true">
                                        <span class="txt_mail"><%- from.addr %></span>
                                        <span class="img_mail ico_more">보내는사람 변경</span>
                                    </a>
                                    <!-- 보내는 사람 이름확인 레이어 -->
                                    <div class="layer_namecheck" id="guideLayer" style="display: none;">
                                        <img src="//mail1.daumcdn.net/mail_static/mint/img/svc/img_namecheck_170109.png" class="" alt="">
                                        <strong class="screen_out">보내는 사람을 변경하려면 여기를 클릭해주세요!</strong>
                                            <a href="/#setting/Accounts" class="link_setting">보내는 사람 변경하러 가기</a>
                                            <a href="javascript:;" class="btn_close">닫기</a>
                                    </div>
                                    <!-- // 보내는 사람 이름확인 레이어 -->
                                </div>
                            </div>
                            <div class="layer_suggest">
                                <ul class="list_suggest list_accounts"></ul>
                                <div class="footer_suggest">
                                    <div class="inner_footer">
                                        <a href="/#setting/Accounts" class="link_opt">기본 메일 주소 / 이름 변경<span class="img_mail ico_arrow"></span></a>
                                    </div>
                                </div>
                            </div>
                        </dd>
                        <dt class="tit_from">받는사람</dt>
                        <dd class="desc_from desc composer_from"><!--  작성시 write_on 클래스 추가  -->
                            <!--  서제스트 활성화 suggest_on 클래스추가 -->
                            <div class="desc_write connectedSortable">
                                <div class="from_opt" aria-hidden="true">
