    var initFromDate = fromDate ? moment(fromDate, "YYYYMMDD") : moment();
    var initToDate = toDate ? moment(toDate, "YYYYMMDD") : moment();
    %>
    <h4 class="screen_out">부재중 응답</h4>
    <dl class="list_basic fst <% if(!enabled){ %>not_use<% }%>">
        <dt class="tit_basic fst">부재중 응답 사용</dt>
        <dd class="txt_basic fst">
            <div class="radio_mail <% if(enabled){ %>on<% }%>"><!-- 클릭 시 클래스 on추가 / 포커스시 클래스 focused추가 -->
                <input type="radio" name="enabled" id="absenceCK1" class="inp_radio" <% if(enabled){ %>checked<% } %> value="true">
                <label for="absenceCK1" class="lab_radio">
                    <span class="img_mail ico_radio"></span>
                    <span class="lab_txt">사용함</span>
                </label>
            </div>
            <div class="radio_mail <% if(!enabled){ %>on<% }%>">
                <input type="radio" name="enabled" id="absenceCK2" class="inp_radio" <% if(!enabled){ %>checked<% } %> value="false">
                <label for="absenceCK2" class="lab_radio">
                    <span class="img_mail ico_radio"></span>
                    <span class="lab_txt">사용 안 함</span>
                </label>
            </div>
            <p class="txt_info info_type2">부재중에 메일을 보낸 사람에게 자동으로 답장을 보냅니다.</p>
        </dd>
    </dl>
    <dl class="list_basic list_absence" style="<% if(!enabled){ %>display:none;<% }%>">
        <dt class="tit_basic tit_period">부재 기간</dt>
        <dd class="txt_basic cont_period">
            <div class="box_datechoice">
                <input type="hidden" id="fromDate" name="fromDate" value="<%= initFromDate.format("YYYYMMDD") %>">
                <div class="write_place absence_start" style="width:101px" tabindex="0">
                    <label for="fromDatePlaceholder" class="screen_out">시작 날짜</label>
                    <input type="text" class="inp_txt" id="fromDatePlaceholder" value="<%= initFromDate.format("YYYY.MM.DD") %>" data-value="fromDate" maxlength="10">
                </div>

                <span class="txt_wave">~</span>

                <input type="hidden" id="toDate" name="toDate" value="<%= initToDate.format("YYYYMMDD") %>">
                <div class="write_place absence_end" style="width:101px" tabindex="0">
                    <label for="toDatePlaceholder" class="screen_out">끝나는 날짜</label>
                    <input type="text" class="inp_txt" id="toDatePlaceholder" value="<%= initToDate.format("YYYY.MM.DD") %>" data-value="toDate" maxlength="10">
                </div>
            </div>
        </dd>
        <dt class="tit_basic tit_text"><label for="absenceReply" class="lab_default">답장 내용</label></dt>
        <dd class="txt_basic cont_text">
            <div class="box_textarea">
                <textarea name="text" id="absenceReply" class="txt_reply"><%= text %></textarea>
            </div>
        </dd>
        <dt class="tit_basic tit_preview tit_preview">미리보기</dt>
        <dd class="txt_basic tit_preview">
            <div class="box_preview">
                <div class="preview_top">
                    <strong class="tit_absence">지금은 부재중입니다.</strong>

                    <p><%- defaultAccount.name %>(<%- defaultAccount.addr %>) 님은 현재 부재중입니다.</p>
                    <dl class="info_date">
                        <dt>부재중 기간 : </dt>
                        <dd><span class="start_date_txt"><%= initFromDate.format("YYYY.MM.DD") %></span> ~ <span class="end_date_txt"><%= initToDate.format("YYYY.MM.DD") %></span></dd>
                    </dl>
                    <em class="emph_preview">이 메일은 부재중 응답 설정을 통해 전송되는 메일입니다.</em>

                    <div class="img_setup bg_absence"></div>
                </div>
                <p class="txt_absence"><%= text %></p>
            </div>
        </dd>
    </dl>
    <div class="btn_box">
            <span class="inner_btn">
                <button type="button" class="btn_g btn_check btn_save disabled" disabled="disabled"><span class="txt">저장</span></button>
                <button type="button" class="btn_g btn_cancel" disabled="disabled"><span class="txt">취소</span></button>
            </span>
    </div>
</script>
