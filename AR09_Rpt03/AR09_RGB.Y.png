                    <span class="txt_mailaddr">
                    <% for(var i=0,len=toList.length;i<len;i++){ %>
                        <a href="javascript:;" class="link_mailaddr" data-name="<%- toList[i].name %>" data-addr="<%= toList[i].addr %>"><%- toList[i].name %> &lt;<%= toList[i].addr %>&gt;</a><% if(i < len-1){ %>,<% } %>
                    <% }%>
                    </span>
                    <a href="javascript:;" class="link_add" data-value="toList">주소추가</a>
                </dd>
                <% } %>
                <% if(ccList.length > 0){ %>
